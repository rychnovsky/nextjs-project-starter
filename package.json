{
  "name": "nextjs-project-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write \"*/**/*.{js,ts,tsx,css,md,json}\"",
    "format:check": "prettier --check \"*/**/*.{js,ts,tsx,css,md,json}\"",
    "lint:ts": "next lint",
    "lint:css": "stylelint \"src/**/*.ts\"",
    "test": "jest",
    "type-check": "tsc --pretty --noEmit",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "next": "^15.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "22.9.0",
    "@types/react": "18.3.12",
    "@types/styled-components": "^5.1.34",
    "eslint": "9.14.0",
    "eslint-config-next": "^15.0.3",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "postcss-styled-syntax": "^0.7.0",
    "prettier": "^3.3.3",
    "stylelint": "^16.10.0",
    "stylelint-config-recommended": "^14.0.1",
    "typescript": "5.6.3"
  },
  "packageManager": "yarn@4.1.1"
}
