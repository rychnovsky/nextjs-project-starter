{
  "name": "nextjs-project-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write \"*/**/*.{js,ts,tsx,css,md,json}\"",
    "format:check": "prettier --check \"*/**/*.{js,ts,tsx,css,md,json}\"",
    "lint:ts": "next lint",
    "lint:css": "stylelint \"src/**/*.ts\"",
    "test": "jest",
    "type-check": "tsc --pretty --noEmit",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "next": "^14.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@types/eslint": "^8",
    "@types/jest": "^29.5.12",
    "@types/node": "20.12.7",
    "@types/react": "18.2.79",
    "@types/styled-components": "^5.1.34",
    "eslint": "8.57.0",
    "eslint-config-next": "^14.2.2",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "postcss-styled-syntax": "^0.6.4",
    "prettier": "^3.2.5",
    "stylelint": "^16.3.1",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "5.4.5"
  },
  "packageManager": "yarn@4.1.1"
}
